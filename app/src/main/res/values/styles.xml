<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="logostyle">
        <item name="android:elevation">2dp</item>
        <item name="android:fontFamily">@font/logofont1</item>
        <item name="android:letterSpacing">0.3</item>
        <item name="android:shadowColor">#40000000</item>
        <item name="android:shadowDx">7</item>
        <item name="android:shadowDy">9</item>
        <item name="android:shadowRadius">4</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">#FF8750</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="sign_up_description">
        <item name="android:layout_width">326dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:fontFamily">@font/m_medium</item>
        <item name="android:textColor">@color/secondary_text_color</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="tool_bar_login">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/darkorange_color</item>
        <item name="android:minHeight">?attr/actionBarSize</item>
        <item name="android:theme">?attr/actionBarTheme</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent = "ThemeOverlay.AppCompat.Dark.ActionBar">
    </style>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
</resources>